#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x5e92372538b0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x5e9237263b40 .scope module, "testbench" "testbench" 3 1;
 .timescale -9 -12;
v0x5e9237279860_0 .var "D", 7 0;
v0x5e9237279950_0 .net/real "Vout", 0 0, L_0x5e92372534f0;  1 drivers
v0x5e9237279a20_0 .var/real "Vref", 0 0;
v0x5e9237279b20_0 .var/i "fh", 31 0;
v0x5e9237279bc0_0 .var/i "i", 31 0;
S_0x5e9237263cd0 .scope module, "uut" "dac8bit" 3 9, 4 3 0, S_0x5e9237263b40;
 .timescale -9 -12;
    .port_info 0 /INPUT 8 "D";
    .port_info 1 /INPUT 1 "Vref";
    .port_info 2 /OUTPUT 1 "Vout";
L_0x5e92372534f0 .functor BUFZ 1, v0x5e9237279680_0, C4<0>, C4<0>, C4<0>;
v0x5e923722bba0_0 .net "D", 7 0, v0x5e9237279860_0;  1 drivers
v0x5e92372795c0_0 .net/real "Vout", 0 0, L_0x5e92372534f0;  alias, 1 drivers
v0x5e9237279680_0 .var/real "Vout_reg", 0 0;
v0x5e9237279720_0 .net/real "Vref", 0 0, v0x5e9237279a20_0;  1 drivers
E_0x5e9237262f60 .event edge, v0x5e9237279720_0, v0x5e923722bba0_0;
    .scope S_0x5e9237263cd0;
T_0 ;
    %wait E_0x5e9237262f60;
    %load/real v0x5e9237279720_0;
    %load/vec4 v0x5e923722bba0_0;
    %cvt/rv;
    %mul/wr;
    %pushi/real 2139095040, 4073; load=255.000
    %div/wr;
    %store/real v0x5e9237279680_0;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5e9237263b40;
T_1 ;
    %pushi/real 1342177280, 4068; load=5.00000
    %store/real v0x5e9237279a20_0;
    %end;
    .thread T_1, $init;
    .scope S_0x5e9237263b40;
T_2 ;
    %vpi_func 3 16 "$fopen" 32, "dac8bit.csv", "w" {0 0 0};
    %store/vec4 v0x5e9237279b20_0, 0, 32;
    %load/vec4 v0x5e9237279b20_0;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_2.0, 4;
    %vpi_call/w 3 18 "$display", "Error opening dac8bit.csv" {0 0 0};
    %vpi_call/w 3 19 "$finish" {0 0 0};
T_2.0 ;
    %vpi_call/w 3 21 "$fdisplay", v0x5e9237279b20_0, "code,vout" {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5e9237279bc0_0, 0, 32;
T_2.2 ;
    %load/vec4 v0x5e9237279bc0_0;
    %cmpi/s 256, 0, 32;
    %jmp/0xz T_2.3, 5;
    %load/vec4 v0x5e9237279bc0_0;
    %pad/s 8;
    %store/vec4 v0x5e9237279860_0, 0, 8;
    %delay 1000, 0;
    %vpi_call/w 3 24 "$fdisplay", v0x5e9237279b20_0, "%0d,%.12f", v0x5e9237279860_0, v0x5e9237279950_0 {0 0 0};
    %load/vec4 v0x5e9237279bc0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5e9237279bc0_0, 0, 32;
    %jmp T_2.2;
T_2.3 ;
    %vpi_call/w 3 26 "$fclose", v0x5e9237279b20_0 {0 0 0};
    %vpi_call/w 3 27 "$display", "Simulation complete. CSV written to dac8bit.csv" {0 0 0};
    %vpi_call/w 3 28 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "tb/testbench.sv";
    "src/dac8bit.sv";
